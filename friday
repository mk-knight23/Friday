#!/bin/bash

# Friday AI Agent Launcher

# Load environment variables if .env exists in the agent directory
AGENT_DIR="/Users/mkazi/Downloads/Friday"
if [ -f "$AGENT_DIR/.env" ]; then
    export $(grep -v '^#' "$AGENT_DIR/.env" | xargs)
fi

# Run the Friday AI Agent
python3 "$AGENT_DIR/main.py" "$@"
